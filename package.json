{
	"name": "github-tada",
	"version": "1.2.3",
	"description": "Browse Github issues by reaction",
	"author": "Moritz Jacobs <mail@moritzjacobs.de>",
	"license": "MIT",
	"scripts": {
		"test": "yarn test:unit && yarn test:lint && yarn test:types",
		"test:types": "tsc -p tsconfig.json --noEmit true",
		"test:unit": "jest",
		"test:watch": "jest --watch",
		"test:lint": "eslint ./src",
		"lint:fix": "eslint ./src --fix",
		"dev": "webpack -w & web-ext run -t firefox-desktop -t chromium",
		"chrome:dev": "webpack -w & web-ext run -t chromium",
		"ff:dev": "webpack -w & web-ext run",
		"build": "rm -rf dist/ && rm -rf ./web-ext-artifacts && yarn webpack && web-ext build -o && yarn publish-src",
		"publish-src": "zip -r ../github-tada-src.zip ./ -x ./.scripts/\\* ./dist/\\* ./node_modules/\\* ./web-ext-artifacts/\\* ./.git/\\*"
	},
	"devDependencies": {
		"@types/jest": "24.0.23",
		"@types/react": "^16.9.11",
		"@types/react-dom": "16.9.4",
		"@types/react-test-renderer": "^16.9.1",
		"@typescript-eslint/eslint-plugin": "2.7.0",
		"@typescript-eslint/parser": "2.7.0",
		"css-loader": "3.2.0",
		"eslint": "^6.6.0",
		"eslint-config-peerigon": "^24.0.0",
		"eslint-import-resolver-typescript": "2.0.0",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-prefer-arrow": "1.1.6",
		"eslint-plugin-react": "7.16.0",
		"eslint-plugin-react-hooks": "2.3.0",
		"jest": "^24.9.0",
		"mini-css-extract-plugin": "0.8.0",
		"node-sass": "4.13.0",
		"react-test-renderer": "16.12.0",
		"sass-loader": "8.0.0",
		"style-loader": "1.0.0",
		"ts-jest": "^24.1.0",
		"ts-loader": "6.2.1",
		"typescript": "3.7.2",
		"web-ext": "3.2.1",
		"webpack": "4.41.2",
		"webpack-cli": "3.3.10"
	},
	"dependencies": {
		"react": "16.12.0",
		"react-dom": "16.12.0"
	}
}
