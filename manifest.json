{
	"name": "issue-yay-nay",
	"version": "1.0",
	"description": "Browse Github issues by reaction",
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'; font-src 'self' data:;",
	"content_scripts": [
		{
			"js": [
				"dist/main.js"
			],
			"matches": [
				"https://github.com/*/issues/*"
			]
		}
	],
	"manifest_version": 2
}