{
	"name": "github-tada",
	"version": "1.1",
	"description": "Browse Github issues by reaction",
	"content_scripts": [
		{
			"js": [
				"../dist/main.js"
			],
			"csss": [
				"../dist/main.css"
			],
			"matches": [
				"https://github.com/*/issues/*",
				"https://github.com/*/pull/*"
			]
		}
	],
	"manifest_version": 2
}